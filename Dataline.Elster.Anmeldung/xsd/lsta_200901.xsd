ame="Kz72" type="elster:lsta_DefaultKZSTypewithCent" minOccurs="0"/>						<xs:element name="Kz73" type="elster:lsta_DefaultKZSTypewithCent" minOccurs="0"/>						<xs:element name="Kz74" type="elster:lsta_DefaultKZSTypewithCent" minOccurs="0"/>						<xs:element name="Kz76" type="elster:lsta_DefaultKZSTypewithCent" minOccurs="0"/>						<xs:element name="Kz77" type="elster:lsta_DefaultKZSTypewithCent" minOccurs="0"/>						<xs:element name="Kz78" type="elster:lsta_DefaultKZSTypewithCent" minOccurs="0"/>						<xs:element name="Kz83" type="elster:lsta_DefaultKZSTypewithCent">							<xs:annotation>								<xs:documentation xml:lang="DE">Die Kz 83 ist eine Pflichtkennzahl und muss übermittelt werden.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="Kz86" type="elster:Kz86"/>					</xs:all>				</xs:complexType>			</xs:element>		</xs:sequence>	</xs:complexType></xs:schema>