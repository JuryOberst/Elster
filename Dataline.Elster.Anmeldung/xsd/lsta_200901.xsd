<?xml version="1.0" encoding="ISO-8859-15"?><!-- Version 2009.1 --><xs:schema xmlns:elster="http://www.elster.de/2002/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.elster.de/2002/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:complexType name="AnmeldungssteuernCType">		<xs:sequence>			<xs:element name="DatenLieferant" type="elster:lstaDatenlieferantType"/>			<xs:element name="Erstellungsdatum" type="elster:MyDateSType"/>			<xs:element name="Steuerfall" type="elster:lstaSteuerfallType"/>		</xs:sequence>		<xs:attribute name="art" use="required" fixed="LStA">			<xs:simpleType>				<xs:restriction base="xs:string"/>			</xs:simpleType>		</xs:attribute>		<xs:attribute name="version" use="required">			<xs:simpleType>				<xs:restriction base="xs:string">					<xs:enumeration value="200901"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>	</xs:complexType>	<xs:complexType name="AdresseCType">		<xs:sequence>			<xs:element name="Bezeichnung" type="elster:BezeichnungSType" minOccurs="0"/>			<xs:element name="Name" type="elster:NameSType" minOccurs="0"/>			<xs:element name="Vorname" type="elster:VornameSType" minOccurs="0"/>			<xs:element name="Namensvorsatz" type="elster:NamenszusatzSType" minOccurs="0"/>			<xs:element name="Namenszusatz" type="elster:NamenszusatzSType" minOccurs="0"/>			<xs:element name="Str" type="elster:StrSType" minOccurs="0"/>			<xs:element name="Hausnummer" type="elster:HausnummerSType" minOccurs="0"/>			<xs:element name="HNrZusatz" type="elster:HNrZusatzSType" minOccurs="0"/>			<xs:element name="AnschriftenZusatz" type="elster:AnschriftenZusatzSType" minOccurs="0"/>			<xs:element name="Ort" type="elster:OrtSType" minOccurs="0"/>			<xs:element name="PLZ" type="elster:PLZSType" minOccurs="0"/>			<xs:element name="AuslandsPLZ" type="elster:AuslandsPLZSType" minOccurs="0"/>			<xs:element name="Land" type="elster:LandSType" minOccurs="0"/>			<xs:element name="PostfachOrt" type="elster:OrtSType" minOccurs="0"/>			<xs:element name="Postfach" type="elster:PostfachSType" minOccurs="0"/>			<xs:element name="PostfachPLZ" type="elster:PLZSType" minOccurs="0"/>			<xs:element name="GKPLZ" type="elster:PLZSType" minOccurs="0"/>			<xs:element ref="elster:Telefon" minOccurs="0"/>			<xs:element name="Email" type="elster:EmailSType" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="MandantCType">		<xs:sequence>			<xs:element name="Name" type="elster:NameSType" minOccurs="0"/>			<xs:element name="Vorname" type="elster:VornameSType" minOccurs="0"/>			<xs:element name="MandantenNr" type="elster:MandantenNrSType" minOccurs="0"/>			<xs:element name="Bearbeiterkennzeichen" type="elster:BearbeiterkennzeichenSType" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<!--- simple Elements & Types ____________________________________________________________ -->	<xs:simpleType name="AnschriftenZusatzSType">		<xs:restriction base="xs:string">			<xs:maxLength value="40"/>			<xs:minLength value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="AuslandsPLZSType">		<xs:restriction base="xs:string">			<xs:maxLength value="13"/>			<xs:minLength value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="BearbeiterkennzeichenSType">		<xs:restriction base="xs:string">			<xs:maxLength value="20"/>			<xs:minLength value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="BezeichnungSType">		<xs:restriction base="xs:string">			<xs:maxLength value="50"/>			<xs:minLength value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="EmailSType">		<xs:restriction base="xs:string">			<xs:maxLength value="70"/>			<xs:minLength value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="HausnummerSType">		<xs:restriction base="xs:string">			<xs:pattern value="\d{1,9}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="HNr