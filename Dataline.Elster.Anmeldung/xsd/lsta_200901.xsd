n value="04"/>			<xs:enumeration value="05"/>			<xs:enumeration value="06"/>			<xs:enumeration value="07"/>			<xs:enumeration value="08"/>			<xs:enumeration value="09"/>			<xs:enumeration value="10"/>			<xs:enumeration value="11"/>			<xs:enumeration value="12"/>			<xs:enumeration value="19"/>			<xs:enumeration value="41"/>			<xs:enumeration value="42"/>			<xs:enumeration value="43"/>			<xs:enumeration value="44"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="allg_StNrSType">		<xs:annotation>			<xs:documentation xml:lang="DE">gemäß Elster Developers Toolkit</xs:documentation>			<xs:documentation xml:lang="DE">13 stellig Nummerisch</xs:documentation>			<xs:documentation xml:lang="DE">Bundesfinanzamtsnummer + 0 + Bezirk + Unterscheidungsnr. + Prüfziffer</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="\d{4}0\d{8}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="lsta_DefaultKZSTypewithCent">		<xs:annotation>			<xs:documentation xml:lang="DE">Vorzeichen: es ist nur das "-" als Vorzeichen zulässig "+" nicht !!!</xs:documentation>			<xs:documentation xml:lang="DE">11 Stellen (Ziffern) vor dem Dezimaltrennzeichen</xs:documentation>			<xs:documentation xml:lang="DE">Dezimaltrennzeichen ist der "."</xs:documentation>			<xs:documentation xml:lang="DE">2 Stellen nach dem Dezimaltrennzeichen</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:maxLength value="15"/>			<xs:minLength value="4"/>			<xs:pattern value="[\-]{0,1}\d{1,11}[.]\d{2}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="lstaDefaultKZSType">		<xs:restriction base="xs:string">			<xs:maxLength value="14"/>			<xs:minLength value="1"/>			<xs:pattern value="[\-]{0,1}\d{1,13}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="lsta_Kz09SType">		<xs:restriction base="xs:string">			<xs:maxLength value="95"/>			<xs:pattern value="([0-9]{5})(\*([^\p{C}*]{0,85})\*([^\p{C}*]{0,85})\*([^\p{C}*]{0,85})\*([^\p{C}*]{0,85})\*([^\p{C}*]{0,85}))?"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="MyDateSType">		<xs:annotation>			<xs:documentation xml:lang="DE">JJJJMMDD</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="\d{4}((0[1-9])|(1[0-2]))((3[0-1])|(2[0-9])|(1[0-9])|(0[1-9]))"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Kz10">		<xs:restriction base="xs:string">			<xs:enumeration value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Kz26">		<xs:restriction base="xs:string">			<xs:enumeration value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Kz29">		<xs:restriction base="xs:string">			<xs:enumeration value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Kz86">		<xs:restriction base="xs:string">			<xs:maxLength value="7"/>			<xs:minLength value="1"/>			<xs:pattern value="\d{1,7}"/>		</xs:restriction>	</xs:simpleType>	<xs:element name="Name">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:minLength value="1"/>				<xs:maxLength value="45"/>				<xs:pattern value="[^\s\p{C}][^\p{C}]*"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="Strasse">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:minLength value="1"/>				<xs:maxLength value="30"/>				<xs:pattern value="[^\s\p{C}][^\p{C}]*"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="PLZ">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="12"/>				<xs:minLength value="1"/>				<xs:pattern value="[^\s\p{C}][^\p{C}]*"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="Ort">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="30"/>				<xs:minLength value="1"/>				<xs:pattern value="[^\s\p{C}][^\p{C}]*"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="Telefon">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="20"/>				<xs:minLength value="1"/>				<xs:pattern value="[^\s\p{C}][^\p{C}]*"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="Email">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="70"/>				<xs:minLength value="1"/>				<xs:pattern value="[^\s\p{C}][^\p{C}]*"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<!--- complex  Elements & Types _______________________________________________________________ -->	<xs:complexType name="LStACType">		<xs:sequence>			<xs:element name="Anmeldungssteuern" type="elster:AnmeldungssteuernCType"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="lstaDatenlieferantType">		<xs:sequence>			<xs:element ref="elster:Name"/>			<xs:element ref="elster:Strasse"/>			<xs:element ref="elster:PLZ"/>			<xs:element ref="elster:Ort"/>			<xs:element ref="elster:Telefon" minOccurs="0"/>			<xs:element ref="elster:Email" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="lstaSteuerfallType">		<xs:sequence>			<xs:element name="Berater" type="elster:AdresseCType" minOccurs="0"/>			<xs:element name="Mandant" type="elster:MandantCType" minOccurs="0"/>			<xs:element name="Unternehmer" type="elster:AdresseCType" minOccurs="0"/>			<xs:element name="Lohnsteueranmeldung">				<xs:complexType>					<xs:all>						<xs:element name="Jahr" type="elster:lstaJahrSType"/>						<xs:element name="Zeitraum" type="elster:lstaZeitraumSType"/>						<xs:element name="Steuernummer" type="elster:allg_StNrSType"/>						<xs:element name="Kz09" type="elster:lsta_Kz09SType">							<xs:annotation>								<xs:documentation xml:lang="DE">Die Kz 09 ist eine Pflichtkennzahl und muss übermittelt werden</xs:documentation>								<xs:documentation xml:lang="DE">Kz 09 baut sich aus folgenden Informationen zusammen: HerstellerID*Name des Berater*Berufsbezeichnung*TelNr Berater