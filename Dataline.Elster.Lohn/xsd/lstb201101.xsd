<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:elsterbase="http://www.elster.de/basis08/XMLSchema" xmlns:elsterlohn="http://www.elsterlohn.de/2011-01/XMLSchema" targetNamespace="http://www.elsterlohn.de/2011-01/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!-- Version 1-->
	<!-- RootElement = Lohnsteuerbescheinigung 2011_________________________________________________-->
	<xs:import namespace="http://www.elster.de/basis08/XMLSchema" schemaLocation="./allg000008.xsd"/>
	<xs:simpleType name="lstb_LohnsteuerbescheinigungVersionSType">
		<xs:restriction base="elsterbase:allg_BaseStringSType">
			<xs:length value="6"/>
			<xs:enumeration value="201101"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="lstb_LohnsteuerbescheinigungJahrSType">
		<xs:restriction base="xs:positiveInteger">
			<xs:enumeration value="2011"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Definierung einfacher Elements________________________________________________________-->
	<!-- K -->
	<xs:simpleType name="lstb_KinderSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Anzahl der Kinderfreibeträge</xs:documentation>
			<xs:documentation xml:lang="DE">Dezimaltrennzeichen = "."</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="3"/>
			<xs:fractionDigits value="1" fixed="true"/>
			<xs:minInclusive value="0.0"/>
			<xs:maxInclusive value="99.5"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- T -->
	<xs:simpleType name="lstb_Text200">
		<xs:restriction base="elsterbase:allg_BaseStringLightSType">
			<xs:maxLength value="200"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- W -->
	<xs:simpleType name="lstb_WertSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Einheitlicher Datentyp aller zu bescheigenden Werte</xs:documentation>
			<xs:documentation xml:lang="DE">Vorzeichen: es ist nur das "-" als Vorzeichen zulässig "+" nicht !!!</xs:documentation>
			<xs:documentation xml:lang="DE">13 Stellen (Ziffern) vor dem Dezimaltrennzeichen</xs:documentation>
			<xs:documentation xml:lang="DE">Dezimaltrennzeichen ist der "."</xs:documentation>
			<xs:documentation xml:lang="DE">2 Stellen nach dem Dezimaltrennzeichen</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="15"/>
			<xs:fractionDigits value="2" fixed="true"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Definierung globaler complex Types __________________________________________________ -->
	<!-- A -->
	<xs:complexType name="lstb_AdresseCType">
		<xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Hausadresse</xs:documentation>
				</xs:annotation>
				<xs:element name="Str" type="elsterbase:allg_StrSType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Strasse</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Hausnummer" type="elsterbase:allg_HausnummerSType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Hausnummer ohne Zusätze</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HNrZusatz" type="elsterbase:allg_HausnummernZusatzSType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Hausnummernzusatz</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Anschriftenzusatz" type="elsterbase:allg_AnschriftenZusatzSType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>weitere Anschriftenzusätze</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Ort" type="elsterbase:allg_OrtSType">
					<xs:annotation>
						<xs:documentation>Ort</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="PLZ" type="elsterbase:allg_PLZSType">
						<xs:annotation>
							<xs:documentation>Inlandspostleitzahl (Deutschland)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AuslandsPLZ" type="elsterlohn:AuslandsPLZCType">
						<xs:annotation>
							<xs:documentation>0-10 -stellige Auslandspostleitzahl</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:annotation>
					<xs:documentation>Postfachadresse</xs:documentation>
				</xs:annotation>
				<xs:element name="PostfachOrt" type="elsterbase:allg_OrtSType"/>
				<xs:element name="Postfach" type="elsterbase:allg_PostfachSType"/>
				<xs:element name="PostfachPLZ" type="elsterbase:allg_PLZSType"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lstb_ArbGAdresseCType">
		<xs:choice>
			<xs:sequence>
				<xs:element name="Str" type="elsterbase:allg_StrSType" minOccurs="0"/>
				<xs:element name="Hausnummer" type="elsterbase:allg_HausnummerSType" minOccurs="0"/>
				<xs:element name="HNrZusatz" type="elsterbase:allg_HausnummernZusatzSType" minOccurs="0"/>
				<xs:element name="Anschriftenzusatz" type="elsterbase:allg_AnschriftenZusatzSType" minOccurs="0"/>
				<xs:element name="Ort" type="elsterbase:allg_OrtSType"/>
				<xs:choice>
					<xs:element name="PLZ" type="elsterbase:allg_PLZSType"/>
					<xs:element name="AuslandsPLZ" type="elsterlohn:AuslandsPLZCType"/>
				</xs:choice>
				<xs:sequence minOccurs="0">
					<xs:element name="PostfachOrt" type="elsterbase:allg_OrtSType"/>
					<xs:choice>
						<xs:sequence>
							<xs:element name="Postfach" type="elsterbase:allg_PostfachSType"/>
							<xs:element name="PostfachPLZ" type="elsterbase:allg_PLZSType"/>
						</xs:sequence>
						<xs:element name="GKPLZ" type="elsterbase:allg_PLZSType"/>
					</xs:choice>
				</xs:sequence>
			</xs:sequence>
			<xs:sequence>
				<xs:element name="PostfachOrt" type="elsterbase:allg_OrtSType"/>
				<xs:choice>
					<xs:sequence>
						<xs:element name="Postfach" type="elsterbase:allg_PostfachSType"/>
						<xs:element name="PostfachPLZ" type="elsterbase:allg_PLZSType"/>
					</xs:sequence>
					<xs:element name="GKPLZ" type="elsterbase:allg_PLZSType"/>
				</xs:choice>
			</xs:sequence>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="lstb_ArbeitgeberCType">
		<xs:sequence>
			<xs:element name="ArbGName">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Name des AG der die Lohnsteuer abgeführt hat</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="elsterbase:allg_BaseStringLightSType">
						<xs:minLength value="1"/>
						<xs:maxLength value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ArbGFANr">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Bundesfinanzamtsnummer des Finanzamtes an das die Lohnsteuer abgeführt wurde</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="elsterbase:allg_BaseStringSType">
						<xs:length value="4"/>
						<xs:pattern value="28\d{2}"/>
						<xs:pattern value="91\d{2}"/>
						<xs:pattern value="92\d{2}"/>
						<xs:pattern value="11\d{2}"/>
						<xs:pattern value="30\d{2}"/>
						<xs:pattern value="24\d{2}"/>
						<xs:pattern value="22\d{2}"/>
						<xs:pattern value="26\d{2}"/>
						<xs:pattern value="40\d{2}"/>
						<xs:pattern value="23\d{2}"/>
						<xs:pattern value="27\d{2}"/>
						<xs:pattern value="10\d{2}"/>
						<xs:pattern value="32\d{2}"/>
						<xs:pattern value="31\d{2}"/>
						<xs:pattern value="21\d{2}"/>
						<xs:pattern value="41\d{2}"/>
						<xs:pattern value="51\d{2}"/>
						<xs:pattern value="52\d{2}"/>
						<xs:pattern value="53\d{2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ArbGFAName">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Names des Finanzamtes an das die Lohnsteuer abgeführt wurde</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="elsterbase:allg_BaseStringSType">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ArbGStNr" type="elsterbase:allg_StNrSType">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Steuernummer des Arbeitgebers unter der die Lohnsteuer abgeführt wurde im bundeseinheitlichen "Elsterformat" </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Adresse" type="elsterlohn:lstb_ArbGAdresseCType">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Adresse des Arbeitgebers</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lstb_AllgemeinCType">
		<xs:sequence>
			<xs:element name="ETIN" type="elsterbase:allg_ETINSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>eTIN des Arbeitnehmers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdNr" type="elsterbase:allg_IdNrSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>die IdNr nach §139b AO des Arbeitnehmers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Ordnungsmerkmal">
				<xs:annotation>
					<xs:documentation>Ordnungsmerkmal des Arbeitgebers für den Arbeitnehmer z.B. Personalnummer</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="elsterbase:allg_BaseStringLightSType">
						<xs:maxLength value="20"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AGS" type="elsterbase:allg_AGSSType">
				<xs:annotation>
					<xs:documentation>Amtlicher Gemeindeschlüssel in dem der Arbeitnehmer zum 20.09. des Vorjahres gemeldet war</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Person" type="elsterlohn:lstb_PersonCType">
				<xs:annotation>
					<xs:documentation>Personendaten des Arbeitnehmers</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- B -->
	<xs:complexType name="lstb_BesteuerungsmerkmaleCType">
		<xs:sequence>
			<xs:element name="Steuerklasse" maxOccurs="12">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Steuerklassen mit Historie</xs:documentation>
					<xs:documentation xml:lang="DE">die Steuerklasse 0 ist ausschließlich bei Arbeitnehmern mit Hauptwohnsitz im Ausland zulässig, deren Arbeitlohn auf Grund eines DBA steuerfrei ist (besondere Lohnsteuerbescheinigung)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="elsterbase:allg_SteuerklasseSType">
							<xs:attribute name="gueltig_ab" type="elsterbase:allg_TTMM" use="required">
								<xs:annotation>
									<xs:documentation>gültig ab Datum im Format TTMM</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="faktor">
								<xs:annotation>
									<xs:documentation>Die Angabe des Faktors ist ausschließlich in der Lohnsteuerklasse 4 zulässig.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="4"/>
										<xs:fractionDigits value="3" fixed="true"/>
										<xs:maxInclusive value="0.999"/>
										<xs:minInclusive value="0.001"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Kinder" maxOccurs="12">
				<xs:annotation>
					<xs:documentation>Kinder mit Historie</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="elsterlohn:lstb_KinderSType">
							<xs:attribute name="gueltig_ab" type="elsterbase:allg_TTMM" use="required">
								<xs:annotation>
									<xs:documentation>gültig ab Datum im Format TTMM</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Kirchensteuerabzug" maxOccurs="12">
				<xs:annotation>
					<xs:documentation>Kirchensteuerabzug mit Historie</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="konfession" type="elsterbase:allg_Konfession_ab2008SType" use="required">
						<xs:annotation>
							<xs:documentation>Konfession des Arbeitnehmers</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ehegkonfession" type="elsterbase:allg_Konfession_ab2008SType" use="optional">
						<xs:annotation>
							<xs:documentation>abweichende Konfession des Ehegatten des Arbeitnehmers</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="gueltig_ab" type="elsterbase:allg_TTMM" use="required">
						<xs:annotation>
							<xs:documentation>gültig ab Datum im Format TTMM</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Steuerfreibetrag" type="elsterlohn:lstb_SteuerfreibetragCType" minOccurs="0" maxOccurs="12">
				<xs:annotation>
					<xs:documentation>Steuerfreibetrag mit Historie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Hinzurechnungsbetrag" type="elsterlohn:lstb_SteuerfreibetragCType" minOccurs="0" maxOccurs="12">
				<xs:annotation>
					<xs:documentation>Hinzurechnungsbetrag mit Historie</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lstb_BesteuerungsgrundlagenCType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Besteuerungsgrundlagen der LSTK 2008</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AnzahlU" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="DE">2. Zeiträume ohne Anspruch auf Arbeitslohn </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="elsterbase:allg_BaseStringSType">
						<xs:maxLength value="2"/>
						<xs:minLength value="1"/>
						<xs:pattern value="[0-9][0-9]?"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BruttoArbLohn" type="elsterlohn:lstb_WertSType">
				<xs:annotation>
					<xs:documentation xml:lang="DE">3. Bruttoarbeitslohn einsch. Sachbezüge ohne 9. und 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LSteuer" type="elsterlohn:lstb_WertSType">
				<xs:annotation>
					<xs:documentation xml:lang="DE">4. Einbehaltene Lohnsteuer von 3.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Soli" type="elsterlohn:lstb_WertSType">
				<xs:annotation>
					<xs:documentation xml:lang="DE"> 5. - Einbehaltene Solidaritätszuschlag von 3.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArbnKiSteuer" type="elsterlohn:lstb_WertSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="DE"> 6. Einbehaltene Kirchensteuer des Arbeitnehmers von 3.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EhegKiSteuer" type="elsterlohn:lstb_WertSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="DE"> 7. - Einbehaltene Kirchensteuer des Ehegatten von 3. (nur bei konfessionsverschiedener Ehe)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VBez" minOccurs="0" maxOccurs="12">
				<xs:annotation>
					<xs:documentation xml:lang="DE"> 8. - In 3. enthaltene Versorgungsbezüge</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="elsterlohn:lstb_WertSType">
							<xs:attribute name="jahr" type="elsterbase:allg_Datum_JahrSType" use="required">
								<xs:annotation>
									<xs:documentation>Maßgebendes Kalenderjahr in dem die Rechtsgrundlage des Vorsorgungsbezug geschaffen wurde</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="beginn" type="elsterbase:allg_Datum_MMSType" use="optional">
								<xs:annotation>
									<xs:documentation>bei nur zeitweiser Gewährung von laufenden Versorgungsbezügen ist der Beginn und Ende Monat zu bescheinigen</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="ende" type="elsterbase:allg_Datum_MMSType" use="optional">
								<xs:annotation>
									<xs:documentation>bei nur zeitweiser Gewährung von laufenden Versorgungsbezügen ist der Beginn und Ende Monat zu bescheinigen</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="bmgfreibetrag" type="elsterlohn:lstb_WertSType" use="required">
								<xs:annotation>
									<xs:documentation>Bemessungsgrundlage für den Versorgungsfreibtrag</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="einmversbezug" type="elsterlohn:lstb_WertSType" use="optional">
								<xs:annotation>
									<xs:documentation>Einmalige Versorgungsbezüge sind Sonderzahlungen wie z.B. Sterbegeld</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:element name="ErmStVBezMKalJahr" minOccurs="0" maxOccurs="12">
					<xs:annotation>
						<xs:documentation xml:lang="DE">9. - Ermäßigt besteuerte Versorgungsbezüge für mehrere Kalenderjahre</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="elsterlohn:lstb_WertSType">
								<xs:attribute name="jahr" type="elsterbase:allg_Datum_JahrSType" use="required">
									<xs:annotation>
										<xs:documentation>Maßgebendes Kalenderjahr in dem die Rechtsgrundlage des Vorsorgungsbezug geschaffen wurde</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="ErmStBetrMKalJahr" type="elsterlohn:lstb_WertSType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="DE">10. - Ermäßigt besteuerter Arbeitslohn für mehrere Kalenderjahre (ohne 9.) und ermäßigt besteuerte Entschädigungen</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LSteuerMKalJahr" type="elsterlohn:lstb_WertSType">
					<xs:annotation>
						<xs:documentation xml:lang="DE">11. - Einbehaltene Lohnsteuer von 9. und 10.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SoliMKalJahr" type="elsterlohn:lstb_WertSType">
					<xs:annotation>
						<xs:documentation xml:lang="DE">12. - Einbehaltene Solidaritätszuschlag von 9. und 10.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="KiSteuerArbnMKalJahr" type="elsterlohn:lstb_WertSType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="DE">13. - Einbehaltene Kirchensteuer des Arbeitnehmers von 9. und 10.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="KiSteuerEhegMKalJahr" type="elsterlohn:lstb_WertSType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="DE">14. - Einbehaltene Kirchensteuer des Ehegatten von 9. und 10. (nur bei konfessionsverschiedener Ehe)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:element name="KurzArbGeld" type="elsterlohn:lstb_WertSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="DE">15. - Kurzarbeitergeld, Winterausfallgeld, Zuschuss zum Mutterschaftsgeld, Verdienstausfallentschädigung (Infektionsschutzgesetz), Aufstockungsbetrag und Altersteilzuschlag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StFreiArbLohnDBA" type="elsterlohn:lstb_WertSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="DE">16a. - Steuerfreier Arbeitslohn nach Doppelbesteuerungsabkommen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StFreiArbLohnATE" type="elsterlohn:lstb_WertSType" minOccurs="0">
				<xs:annotation>
