	<xs:simpleType name="BaseStringSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Die Einschraenkungen sind
				in der Schnittstellenbeschreibung SST_Arbeitgeber zu entnehmen;"
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringNoLeadingOrTrailingSeparators">
			<xs:pattern value="[\p{L}|\p{P}|\p{Zs}|\p{Nd}]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringLightSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Der String muss
				mindestens 1 Zeichen vom Typ {N} oder {L} beinhalten, zusaetzlich
				gelten die Einschraenkungen des
				BaseStringNoLeadingOrTrailingSeparators
			</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf
				http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringNoLeadingOrTrailingSeparators">
			<xs:pattern value=".*([\p{N}]|[\p{L}]).*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringNoLeadingOrTrailingSeparators">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Das letzte Zeichen darf
				nicht vom Typ {Z} sein, zusaetzlich gelten die Einschraenkungen des
				BaseStringNoLeadingSeparators
			</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf
				http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringNoLeadingSeparators">
			<xs:pattern value=".*[\P{Z}]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringNoLeadingSeparators">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Das erste Zeichen darf
				nicht vom Typ {Z} sein, zusaetzlich gelten die Einschraenkungen des
				BaseStringUltraLightSType
			</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf
				http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringUltraLightSType">
			<xs:pattern value="[\P{Z}].*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringUltraLightSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Alle Zeichen die nicht
				Kategorien C oder Zl gehoeren;
			</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf
				http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([^\n\r\t\p{C}\p{Zl}])*"/>
		</xs:restriction>
	</xs:simpleType>
